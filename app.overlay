/ {
	gpio_keys {
		compatible = "gpio-keys";
		ledyellow: led_yellow {
			gpios = <&gpio0 12 GPIO_ACTIVE_HIGH>;
			label = "LED 1";
		};
	};
		
	aliases {
		led-yellow = &ledyellow;
		lcd-screen = &lcdscreen;
	};
};

&i2c0 {
	lcdscreen: lcd_screen@27{
		compatible = "i2c-device";
		reg = <0x27>;
		label = "LCD_SCREEN";
	};
};